{
  "env": {
    "browser": true,
    "jest": true,
    "es6": true
  },
  "plugins": ["import"],
  "parser": "babel-eslint",
  // "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    // "comma-dangle": ["error", "never"],
    "no-console": "warn",
    "no-eval": "error",
    "import/first": "error"
  },
  "extends": ["plugin:prettier/recommended"]
}
